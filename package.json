{
  "name": "pl-ai-library",
  "version": "1.0.0",
  "description": "A model agnostic library for interacting with LLMs with a minimal abstraction layer",
  "scripts": {
    "build": "rollup -c"
  },
  "author": "Rudy Soliz",
  "license": "Rights Reserved",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.19",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./agents": {
      "import": "./dist/agents/index.mjs",
      "require": "./dist/agents/index.cjs"
    }
  },
  "dependencies": {
    "openai": "^4.100.0"
  }
}
